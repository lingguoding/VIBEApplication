<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="WrapMatch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="MatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="WrapWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MatchMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="MatchAuto">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="AutoMatch">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="WrapAuto">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="AutoWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="WrapMatch.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="WrapMatch.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MatchWrap.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MatchWrap.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="WrapWrap.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="WrapWrap.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MatchMatch.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MatchMatch.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MatchAuto.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MatchAuto.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="AutoMatch.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="AutoMatch.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="WrapAuto.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="WrapAuto.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="AutoWrap.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="AutoWrap.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MatchOne">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <style name="OneMatch">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="HLine" parent="MatchOne">
        <item name="android:background">@color/adapter_line</item>
    </style>

    <style name="HLine.LeftMargin">
        <item name="android:layout_marginLeft">@dimen/adapter_horizontal_margin</item>
    </style>

    <style name="HLine.RightMargin">
        <item name="android:layout_marginRight">@dimen/adapter_horizontal_margin</item>
    </style>

    <style name="HLine.BothMargin">
        <item name="android:layout_marginRight">@dimen/adapter_horizontal_margin</item>
        <item name="android:layout_marginLeft">@dimen/adapter_horizontal_margin</item>
    </style>

    <style name="VLine" parent="OneMatch">
        <item name="android:background">@color/adapter_line</item>
    </style>

    <style name="VLine.TopMargin">
        <item name="android:layout_marginTop">@dimen/adapter_vertical_margin</item>
    </style>

    <style name="VLine.BottomMargin">
        <item name="android:layout_marginBottom">@dimen/adapter_vertical_margin</item>
    </style>

    <style name="VLine.BothMargin">
        <item name="android:layout_marginTop">@dimen/adapter_vertical_margin</item>
        <item name="android:layout_marginBottom">@dimen/adapter_vertical_margin</item>
    </style>


    <style name="Toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    </style>

    <style name="TitleBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">@android:color/white</item>
        <item name="bgatitlebar_leftAndRightTextColor">@color/textPrimary</item>
        <item name="bgatitlebar_leftAndRightTextSize">@dimen/text_size_14</item>
        <item name="bgatitlebar_titleTextColor">@color/textPrimary</item>
        <item name="bgatitlebar_titleTextSize">@dimen/text_size_18</item>
        <item name="bgatitlebar_leftAndRightPadding">@dimen/size_level2</item>
        <item name="bgatitlebar_titleDrawablePadding">@dimen/size_level1</item>
        <item name="bgatitlebar_leftDrawable">@mipmap/ic_black_left_arrow</item>
    </style>

    <style name="TitleBar.NoBack">
        <item name="bgatitlebar_leftDrawable">@null</item>
    </style>

</resources>
